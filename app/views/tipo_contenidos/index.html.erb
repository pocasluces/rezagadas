<p id="notice"><%= notice %></p>

<h1>Listing Tipo Contenidos</h1>

<table>
  <thead>
    <tr>
      <th>Astrofoto</th>
      <th>Metematica</th>
      <th>Otro1</th>
      <th>Otro2</th>
      <th>Otrotxt</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_contenidos.each do |tipo_contenido| %>
      <tr>
        <td><%= tipo_contenido.astrofoto %></td>
        <td><%= tipo_contenido.metematica %></td>
        <td><%= tipo_contenido.otro1 %></td>
        <td><%= tipo_contenido.otro2 %></td>
        <td><%= tipo_contenido.otrotxt %></td>
        <td><%= link_to 'Show', tipo_contenido %></td>
        <td><%= link_to 'Edit', edit_tipo_contenido_path(tipo_contenido) %></td>
        <td><%= link_to 'Destroy', tipo_contenido, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo contenido', new_tipo_contenido_path %>
